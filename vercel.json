{
    "rewrites": [
        {
            "source": "/api/pipefy",
            "destination": "/api/pipefy"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.cjs"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/sync/daily",
            "schedule": "0 9 * * *"
        }
    ]
}